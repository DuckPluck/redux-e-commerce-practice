{
  "name": "@duckpluck/redux-e-commerce-practice",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn server\" \"vite\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "json-server -w ./db.json -p 3001",
    "lint": "eslint -c .eslintrc.json --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "yarn lint -- --fix && yarn csscomb",
    "csscomb": "csscomb -v ./src",
    "csscomb:lint": "csscomb -lv ./src"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "csscomb": "^4.3.0",
    "eslint": "^8.55.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.7.0",
    "json-server": "^1.0.0-alpha.23",
    "postcss": "^8.4.34",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
